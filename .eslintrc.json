{
  "extends": ["airbnb-base", "plugin:node/recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "node/no-unsupported-features/es-syntax": [
      "error",
      {
        "ignores": ["modules"]
      }
    ],
    "node/no-unpublished-import": [
      "error",
      {
        "allowModules": ["@babel/node"]
      }
    ]
  }
}
